{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "enums": ["./src/enums"],
      "helpers/*": ["./src/helpers/*"],
      "plugins/*": ["./src/plugins/*"],
      "stores": ["./src/stores"],
      "typings": ["./src/typings"],
      "utils/*": ["./src/utils/*"],
      "views/*": ["./src/views/*"]
    }
  }
}
